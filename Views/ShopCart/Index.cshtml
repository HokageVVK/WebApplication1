@model ShopCartViewModel
<div class="container">
    @foreach (var el in Model.shopCart.listShipItems)
            {
                <div class="alert alert-warning mt-3">
                    <b>Автомобиль:</b>@el.car.name<br/>
                    <b>Цена:</b>@el.car.price.ToString("c")
                </div>
            }
    <hr/>
    <div class="btn btn-denger" asp-controller="Order">Оплатить</div>
</div>